<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
    <!--  Name must be same as declared in Employee Class -->
	<bean name="empAddress" class="com.springcore.autowiring.Address">
		<property name="street" value="705 Street" />
		<property name="city" value="Kandivali" />
		<property name="pincode" value="1200001" />
	</bean>
	
	<bean name="employee1" class="com.springcore.autowiring.Employee" autowire="byName">
		<property name="empId" value="121212" />
		<property name="empName" value="Ramesh" />
	</bean>
	
</beans>