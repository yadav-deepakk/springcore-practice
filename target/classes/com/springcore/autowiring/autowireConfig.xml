<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
    <!--  Injection will happen byType Employee Class -->
	<bean name="address1" class="com.springcore.autowiring.Address">
		<property name="street" value="67 Street" />
		<property name="city" value="Boisar" />
		<property name="pincode" value="120101" />
	</bean>
	
	<bean name="employee1" class="com.springcore.autowiring.Employee" autowire="byType">
		<property name="empId" value="121212" />
		<property name="empName" value="Ramesh" />
	</bean>
	
</beans>